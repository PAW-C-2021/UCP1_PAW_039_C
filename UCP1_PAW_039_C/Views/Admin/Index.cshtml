@model IEnumerable<UCP1_PAW_039_C.Models.Admin>

@{
    ViewData["Admin"] = "Admin";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayName("Username")
            </th>
            <th>
                @Html.DisplayName("Password")
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Username)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idadmin"><i class="fa fa-marker fa-lg"></i></a>
                <a asp-action="Details" asp-route-id="@item.Idadmin"><i class="fas fa-list"></i></a> |
                <a asp-action="Delete" asp-route-id="@item.Idadmin"><i class=" fa fa-trash-alt fa-lg"></i></a>
            </td>
        </tr>
}
    </tbody>
</table>
